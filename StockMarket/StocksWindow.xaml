<Window x:Class="StockMarket.StocksWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StockMarket"
        mc:Ignorable="d"
        Title="StocksWindow" Height="450" Width="800">
    <Grid>
        <Label Width="200" Height="35" Margin="-300,-200,0,0" Content="Price"/>
        <Label Width="200" Height="35" Margin="-300,-200,0,-100" Content="Symbol"/>
        <Label Width="200" Height="35" Margin="-300,-200,0,-200" Content="Last_Trade_day"/>
        <TextBox Name="Price" Width="200" Height="35" Margin="-100,-200,0,0" Background="Gray" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="25" Foreground="White"/>
        <TextBox Name="Symbol_" Width="200" Height="35" Margin="-100,-200,0,-100" Background="Gray" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="25" Foreground="White"/>
        <TextBox Name="Last_Trade_price" Width="200" Height="35" Margin="-100,-200,0,-200" Background="Gray" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="25" Foreground="White"/>
    </Grid>
</Window>
